 38, "lines_coords": [[[686.0, 1305.0], [952.0, 1305.0], [952.0, 1346.0], [686.0, 1346.0]], [[686.0, 1365.0], [1173.0, 1365.0], [1173.0, 1402.0], [686.0, 1402.0]], [[690.0, 1425.0], [1020.0, 1425.0], [1020.0, 1462.0], [690.0, 1462.0]]], "lines": ["それでは改めまして", "叶実達を共に見届けていただき本当に", "ありがとうございました！"]}, {"box": [1132, 1841, 1675, 2069], "vertical": false, "font_size": 48, "lines_coords": [[[1136.0, 1841.0], [1455.0, 1841.0], [1455.0, 1890.0], [1136.0, 1890.0]], [[1140.0, 1897.0], [1327.0, 1897.0], [1327.0, 1950.0], [1140.0, 1950.0]], [[1132.0, 1953.0], [1620.0, 1957.0], [1620.0, 2010.0], [1132.0, 2006.0]], [[1143.0, 2025.0], [1665.0, 2025.0], [1665.0, 2062.0], [1143.0, 2062.0]]], "lines": ["担当さま、家族・友人", "読者の皆さま", "関わってくださった全ての皆さま", "いつも本当にありがとうございます"]}, {"box": [688, 1905, 1011, 1945], "vertical": false, "font_size": 44, "lines_coords": [[[688.0, 1901.0], [1011.0, 1901.0], [1011.0, 1949.0], [688.0, 1949.0]]], "lines": ["ＳＰｅｃｉａｌｔｈａｎｋｓ"]}, {"box": [513, 2072, 899, 2139], "vertical": false, "font_size": 73, "lines_coords": [[[513.0, 2066.0], [899.0, 2066.0], [899.0, 2145.0], [513.0, 2145.0]]], "lines": ["ＤＬ－Ｒａｗｉｌｅｔ"]}]}